{
  "name": "yuki-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun build:registry && next build",
    "build:registry": "shadcn build && bun ./scripts/fix-regestry.ts",
    "check": "bun lint && bun format && bun typecheck",
    "dev": "next dev --turbopack",
    "format": "prettier --check . --cache --cache-location .cache/prettier",
    "format:fix": "prettier --check . --write --cache --cache-location .cache/prettier",
    "postinstall": "fumadocs-mdx",
    "lint": "eslint . --cache --cache-location .cache/eslintcache",
    "lint:fix": "eslint . --fix --cache --cache-location .cache/eslintcache",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "arktype": "^2.1.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fumadocs-core": "^15.3.4",
    "fumadocs-docgen": "^2.0.0",
    "fumadocs-mdx": "^11.6.5",
    "fumadocs-ui": "^15.3.4",
    "lucide-react": "^0.511.0",
    "next": "^15.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@next/eslint-plugin-next": "^15.3.2",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "shadcn": "^2.5.0",
    "tailwindcss": "^4.1.7",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  }
}
